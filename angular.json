            "assets": ["projects/ff/src/favicon.ico", "projects/ff/src/assets"],
            "styles": ["projects/ff/src/styles.scss"],
            "scripts": [],
            "extraWebpackConfig": "projects/ff/webpack.config.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/ff/tsconfig.app.json", "projects/ff/tsconfig.spec.json", "projects/ff/e2e/tsconfig.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ff/e2e/protractor.conf.js",
            "devServerTarget": "ff:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ff:serve:production"
            }
          }
        }
      }
    },
